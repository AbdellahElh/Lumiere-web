{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=#{SUPABASE_DB_HOST}#;Database=#{SUPABASE_DB_NAME}#;Username=#{SUPABASE_DB_USER}#;Password=#{SUPABASE_DB_PASSWORD}#;Port=5432;SSL Mode=Require;"
  },
  "AllowedHosts": "*",
  "Auth0": {
    "Domain": "#{AUTH0_DOMAIN}#",
    "ClientId": "#{AUTH0_CLIENT_ID}#",
    "ClientSecret": "#{AUTH0_CLIENT_SECRET}#"
  },
  "CORS": {
    "AllowedOrigins": [
      "https://lumiere-frontend.vercel.app",
      "https://*.vercel.app",
      "http://localhost:5000",
      "https://localhost:5001"
    ]
  }
}
